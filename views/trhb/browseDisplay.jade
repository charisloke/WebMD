.row-fluid
  .content-box
    .browse-pane#browse
      - for (var sIndex in browseItems)
        - s = browseItems[sIndex]
        br
        a(href=s.url)
          = s.value
          - if (typeof(s.count) !== 'undefined' && s.count)
            | &nbsp(#{s.count})
